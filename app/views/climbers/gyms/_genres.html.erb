    <table class="table">
      <thead class="thead-lignt">
        <tr>
          <th>ジャンル検索</th>
        </tr>
      </thead>

      <tbody>
        <% genres.each do |genre| %>
          <tr>
            <td>
            <%= link_to gyms_path(genre_id: genre.id) do %>
              <%= attachment_image_tag genre, :genre_image, size: '100x100',format: "jpeg",fallback: "ira13.png" %>
            <% end %>
              <%= genre.genre_name %>
            </td>
          </tr>
          <% end %>
      </tbody>
    </table>